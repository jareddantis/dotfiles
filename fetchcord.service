[Unit]
Description=Display OS info as Discord Rich Presence
Documentation=https://github.com/MrPotatoBobx/FetchCord
After=network.target multi-user.target graphical-session.target

[Service]
Environment=PYTHONUNBUFFERED=1
ExecStart=-/home/jared/.pyenv/versions/fetchcord/bin/fetchcord --nohardware --noshell --fetchcord-config-path /home/jared/.config/fetchcord/fetchcord.conf
Restart=always
RestartSec=10

[Install]
WantedBy=default.target
